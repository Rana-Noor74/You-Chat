<header class="chat-header">
    <a id="back_user_list" class="btn btn-sm btn-icon btn-flush-dark btn-rounded flush-soft-hover back-user-list" href="#">
        <span class="icon">
            <i class="fa fa-angle-left"></i>
        </span>
    </a>
    <div class="media">
        <div class="media-head" id="chatHead">
            <div class="avatar avatar-sm avatar-soft-danger avatar-rounded border border-1 border-danger border-rounded">
                <span class="initial-wrap">W</span>
            </div>
        </div>
        <div class="media-body" id="chatName">
            <div class="user-name" style="width: 500px!important;">Winston Churchil</div>
            <div class="user-status">Active 8min ago</div>
        </div>
    </div>
    <div class="chat-options-wrap">	
        <a class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover dropdown-toggle no-caret d-none d-xl-block" href="#" data-bs-toggle="modal" data-bs-target="#video_call">
            <span class="icon" data-bs-toggle="tooltip" data-bs-placement="top" title="" data-bs-original-title="Block Contact">
                <i class="fa fa-user-times"></i>
            </span>
        </a>
        <a class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover d-none d-xl-block" href="#" data-bs-toggle="modal" data-bs-target="#audio_call">
            <span class="icon" data-bs-toggle="tooltip" data-bs-placement="top" title="" data-bs-original-title="Audio call">
                <i class="fa fa-phone"></i>
            </span>
        </a>
        <a id="chatInfoBtn" class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover chatapp-info-toggle" href="#">
            <span class="icon" data-bs-toggle="tooltip" data-bs-placement="top" title="" data-bs-original-title="Info">
                <i class="fa fa-info"></i>
            </span>
        </a>
        <a class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover hk-navbar-togglable d-none d-xl-block">
            <span class="icon">
                <span class="feather-icon">
                    <i class="fa fa-angle-up"></i>
                </span>
                <span class="feather-icon d-none">
                    <i class="fa fa-angle-down"></i>
                </span>
            </span>
        </a>
        <a class="d-xl-none dropdown-item" href="#" data-bs-toggle="modal" data-bs-target="#audio_call" style="padding: 0px!important; width: 30%!important;">
            <span class="feather-icon dropdown-icon">
                <i class="fa fa-phone text-primary" style="margin-top: 11px!important;"></i>
            </span>
        </a>
    </div>
</header>
