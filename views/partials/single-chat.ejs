<footer class="chat-footer">
    <button class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover flex-shrink-0" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <span class="icon">
            <i class="fa fa-upload"></i>
        </span>
    </button>
    <div class="dropdown-menu">
        <a class="dropdown-item" href="#">
            <div class="d-flex align-items-center" id="uploadgallery">
                <div class="avatar avatar-icon avatar-xs avatar-soft-primary avatar-rounded">
                    <label for="galleryfile-input">
                        <i class="ri-image-line text-primary"></i> Gallery
                    </label>
                    <input id="galleryfile-input" type="file" class="d-none" accept="image/png, image/gif, image/jpeg" multiple="">
                </div>
            </div>
        </a>
    </div>
    <div class="input-group">
        <span class="input-affix-wrapper">
            <input type="text" id="input_msg_send_chatapp" name="send-msg" class="input-msg-send form-control rounded-input" placeholder="Type your message...">
            <span class="input-suffix">
                <button class="btn btn-icon btn-flush-primary btn-rounded btn-send">
                    <span class="icon">
                       <i class="fa fa-paper-plane"></i>
                    </span>
                </button>
            </span>
        </span>
    </div>
    <button class="btn btn-icon btn-flush-dark btn-rounded flush-soft-hover">
        <span class="icon">
            <i class="fa fa-smile" style="font-weight: normal!important;"></i>
        </span>
    </button>
</footer>
