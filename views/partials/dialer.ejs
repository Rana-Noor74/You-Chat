<div class="dialer-box">
    <div class="dialer-box-inner">
        <div class="container pb-4" style="border:1px solid lightgray;border-radius: 30px; margin-top: 50px;">
            <div class="row">
               <div class="col-12">
                    <div class="dialer-input-parent">
                        <input class="effect-1 rounded-input" id="dialer-input" type="text" placeholder="Type Number">
                        <span class="focus-border"></span>
                    </div> 
               </div>
            </div>
            <div class="row">
                <div class="col-4" style="margin-bottom:10px!important">
                    <div class="avatar avatar-sm avatar-soft-primary he-wi avatar-rounded  border-1 border-primary border-rounded">
                        <span class="initial-wrap text-dark dialer-digit" data-key="1">
                            <p class="select-text-disable">1</p>
                            <p class="select-text-disable" style="font-size: 10px!important;"></p>
                        </span>
                    </div>
                </div>
                <div class="col-4" style="margin-bottom:10px!important">
                    <div class="avatar avatar-sm avatar-soft-primary he-wi avatar-rounded  border-1 border-primary border-rounded">
                        <span class="initial-wrap flex-column text-dark dialer-digit" data-key="2">
                            <p class="select-text-disable">2</p>
                            <p class="select-text-disable" style="font-size: 10px!important;">ABC</p>
                        </span>
                    </div>
                </div>
                <div class="col-4" style="margin-bottom:10px!important">
                    <div class="avatar avatar-sm avatar-soft-primary he-wi avatar-rounded border-1 border-primary border-rounded">
                        <span class="initial-wrap flex-column text-dark dialer-digit" data-key="3">
                            <p class="select-text-disable">3</p>
                            <p class="select-text-disable" style="font-size: 10px!important;">DEF</p>
                        </span>
                    </div>
                </div>
