<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head"); %>
</head>
<body>
   	<!-- Wrapper -->
	<div class="hk-wrapper hk-pg-auth bg-primary-dark-3" data-footer="simple">
		<!-- Main Content -->
		<div class="hk-pg-wrapper pt-0 pb-xl-0 pb-5">
			<div class="hk-pg-body pt-0 pb-xl-0">
				<!-- Container -->
				<div class="container-xxl">
					<!-- Row -->
					<div class="row">
						<div class="col-sm-10 position-relative mx-auto">
							<div class="auth-content py-8">
								<form class="w-100">
									<div class="row">
										<div class="col-lg-4 col-md-6 mx-auto">
											<div class="card card-flush bg-transparent">
												<div class="card-body text-center">
													<div class="avatar avatar-xl avatar-rounded  position-relative mb-3">
														<img src="<%= (process.env.SERVER_TYPE == 'STAGING') ? process.env.LOCAL_URL : process.env.LIVE_URL %>/user/dist/img/avatar12.jpg" alt="user" class="avatar-img">
														<div class="badge-icon badge-icon-xxs text-primary position-bottom-end-overflow-1">
															<div class="badge-icon-wrap">
																<i class="ri-lock-fill"></i>
															</div>
															<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 127 127">
																<g data-name="Ellipse 302" transform="translate(8 8)" stroke-width="3">
																<circle cx="55.5" cy="55.5" r="55.5" stroke="currentColor"></circle>
																<circle cx="55.5" cy="55.5" r="59.5" fill="currentColor"></circle>
																</g>
															</svg>
														</div>
													</div>
													<h4 class="text-white">Jim Carry</h4>
													<p class="p-sm mb-4 text-white opacity-55">admistrator@jampack.com</p>
													<div class="row gx-3">
														<div class="form-group col-lg-12">
															<input class="form-control" placeholder="Enter Password" value="" type="password">
														</div>
													</div>
                                                    <div class="row">
                                                        <a href="/">
														    <button class="btn btn-primary" type="button">Sign In</button>
                                                        </a>
													</div>
												</div>
											</div>
										</div>
									</div>
								</form>
							</div>
						</div>
					</div>
					<!-- /Row -->
				</div>
				<!-- /Container -->
			</div>
			<!-- /Page Body -->
		
		</div>
		<!-- /Main Content -->
	</div>
    <!-- /Wrapper -->
    <%- include("../partials/foot"); %>
</body>
</html>